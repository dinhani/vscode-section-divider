{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES2020",
        "removeComments": true,
        "outDir": "out",
        "lib": ["ES2020"],
        "sourceMap": true,
        "rootDir": ".",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "strict": false,
        "noImplicitAny": false
    },
    "exclude": [
        "node_modules",
        ".vscode-test"
    ]
}